<div class="container">
  <div class="row justify-content-md-center">
    <div class="col-md-8">
      <%= render 'form', post: @post %>


      <h1>Feed</h1>
      <% @posts.reverse.each do |post| %>
        <%= render partial: 'post', locals: { post: post } %>
      <% end %>

    </div>
  </div>
</div>
